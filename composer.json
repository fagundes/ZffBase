{
    "name": "fagundes/zff-base",
    "description": "A ZF2 module that gathers a set of classes which are commonly used in several ZF2 projects",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "zf2", "bootstrap 3", "doctrine 2"
    ],
    "homepage": "https://github.com/fagundes/zff-base",
    "authors": [
      {
          "name": "VinÃ­cius Fagundes",
          "email": "mvlacerda@gmail.com"
      }
    ],
    "require": {
      "php": ">=5.4",
      "zendframework/zend-form": "2.5.x",
      "zendframework/zend-inputfilter": "2.5.x",
      "zendframework/zend-mvc": "2.5.x",
      "zendframework/zend-servicemanager": "2.5.x",
      "zendframework/zend-stdlib": "2.5.x",
      "zendframework/zend-validator": "2.5.x",
      "zendframework/zend-view": "2.5.x",
      "doctrine/doctrine-orm-module": "0.*"
    },
    "require-dev" : {
        "phpunit/phpunit" : ">=3.7,<4",
        "phpmd/phpmd" : "1.4.*"
    },
    "autoload": {
        "psr-4": {
            "Zff\\Base\\": "src/"
        }
    },
     "extra": {
        "alias": {
            "dev-master":"0.1.x-dev"
        }
    }
}
